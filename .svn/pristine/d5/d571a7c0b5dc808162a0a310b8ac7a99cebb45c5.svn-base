/********************************      公用元素      ********************************/

a {
    color: #57a000;
    text-decoration: none;
}

/************ 通用按钮 ************/
.button img {
    vertical-align: middle;
    margin-right: 3px;
}

.button {
    font-family: "微软雅黑", Arial, Helvetica, sans-serif;
    display: inline-block;
    background: #F7F8F9;
    border: 1px solid #E7E7E8 !important;
    padding: 3px 6px !important;
    color: #555555 !important;
    font-size: 13px !important;
    cursor: pointer;
    border-radius: 3px;
    margin-right: 5px;
}


/*************** 分页 ***************/

.main-content .pagination {
    text-align: right;
    padding: 10px 20px;
    font-family: "微软雅黑", Arial, Helvetica, sans-serif;
    font-size: 13px;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
}

.pagination a {
    margin: 0 2px;
    padding: 0 5px;
    border: 1px solid #EBF5FE;
    color: black;
}

    .pagination a.number:hover {
        background: #E4F9A8;
        border: 1px solid #BED08E;
    }

    .pagination a.current {
        border: 1px solid #92A5B4;
        background: #D3EFFF;
    }

    .pagination a img {
        border: 1px solid #D7DCE0;
        vertical-align: top;
    }

    .pagination a.swy {
        padding: 0;
    }

        .pagination a.swy:hover {
            background: none;
        }

.pagination span {
    font-size: 13px;
    margin: 0 5px;
    vertical-align: middle;
}


