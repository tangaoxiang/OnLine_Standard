* {
    margin: 0;
    padding: 0;
    font-family: Arial, Helvetica, sans-serif;
}


/********************************      基础样式初始化      ********************************/
body {
    line-height: 24px;
    font: 13px Helvetica Neue,Helvetica,PingFang SC,\5FAE\8F6F\96C5\9ED1,Tahoma,Arial,sans-serif;
}

a {
    color: #333;
    text-decoration: none;
}

    a:hover {
        color: #fff;
    }

    a cite {
        font-style: normal;
        *cursor: pointer;
    }

#body-wrapper {
    margin: 0px;
}

.clear {
    clear: both;
}
