/**************Showcase Style Starts*****************/

.container {
    margin: 50px auto 0 auto;
    width: 1000px;
}

.showcase {
    list-style: none;
    height: 410px;
    background: url(../image/lcbz-hs.png) left bottom no-repeat;
}

    .showcase li {
        float: left;
        width: 111px;
        height: 444px;
        overflow: hidden;
        margin: 0 13px 0 0;
    }

        .showcase li .lsd {
            border: 1px solid green;
        }

        .showcase li.thumb1:hover .dd01 {
            width: 41px;
            height: 42px;
            background: url(../image/dd01.png) center center no-repeat;
            margin: 24px 0 0 40px;
        }

        .showcase li.thumb1:hover .dd02 {
            width: 41px;
            height: 42px;
            background: url(../image/dd02.png) center center no-repeat;
            margin: 24px 0 0 41px;
        }

        .showcase li.thumb1:hover .dd03 {
            width: 38px;
            height: 42px;
            background: url(../image/dd03.png) center center no-repeat;
            margin: 23px 0 0 40px;
        }

        .showcase li.thumb1:hover .dd04 {
            width: 38px;
            height: 42px;
            background: url(../image/dd04.png) center center no-repeat;
            margin: 25px 0 0 42px;
        }

        .showcase li.thumb1:hover .dd05 {
            width: 38px;
            height: 42px;
            background: url(../image/dd05.png) center center no-repeat;
            margin: 23px 0 0 40px;
        }

        .showcase li.thumb1:hover .dd06 {
            width: 38px;
            height: 42px;
            background: url(../image/dd06.png) center center no-repeat;
            margin: 24px 0 0 42px;
        }

        .showcase li.thumb1:hover .dd07 {
            width: 38px;
            height: 42px;
            background: url(../image/dd07.png) center center no-repeat;
            margin: 23px 0 0 41px;
        }

        .showcase li.thumb1:hover .dd08 {
            width: 38px;
            height: 42px;
            background: url(../image/dd08.png) center center no-repeat;
            margin: 23px 0 0 40px;
        }

    .showcase a {
        display: block;
        width: 111px;
        height: 344px;
        overflow: hidden;
        position: relative;
        text-decoration: none;
        font-family: "微软雅黑, 宋体, 黑体";
    }

        .showcase a p {
            background: #59A214;
            color: #fff;
            font-size: 11px;
            padding: 12px 15px;
            position: absolute;
            bottom: -350px;
            line-height: 20px;
            display: block;
            width: 90px;
            cursor: pointer;
            -webkit-transition: all 0.2s ease-out;
            -moz-transition: all 0.2s ease-out;
            -o-transition: all 0.2s ease-out;
            transition: all 0.2s ease-out;
            font-family: "微软雅黑, 宋体, 黑体";
            font-size: 13px;
        }

        .showcase a img {
            width: 109px;
            height: 342px;
            border: 1px solid #b8c2c3;
            border-radius: 2px;
        }

        .showcase a:hover p {
            bottom: 0;
            background: url(../image/dhbj.png) repeat;
        }

        .showcase a:hover img {
            filter: alpha(opacity=50);
            -moz-opacity: 0.5;
            opacity: 0.5;
        }

        .showcase a .title {
            text-align: center;
            font-size: 18px;
            line-height: 25px;
            text-transform: uppercase;
            color: #fff;
            width: 111px;
            position: absolute;
            left: 0px;
            bottom: 0px;
            cursor: pointer;
            -webkit-transition: all 0.2s ease-out;
            -moz-transition: all 0.2s ease-out;
            -o-transition: all 0.2s ease-out;
            transition: all 0.2s ease-out;
        }

