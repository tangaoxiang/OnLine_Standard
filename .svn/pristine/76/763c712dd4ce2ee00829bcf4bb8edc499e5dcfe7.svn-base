/* CSS Document */
body {
    margin: 0px;
    color: #000000;
    font-family: Verdana, Geneva, sans-serif,"宋体";
    font-size: 12px;
}

#loginbg {
    background-image: url(../images/login_01.jpg);
    background-repeat: repeat-x;
}

.input {
    line-height: 19px;
    border: #dcdcdc 1px solid;
    background-image: url(../images/input_bg.gif);
    height: 19px;
}

.border_three {
    border-left: 1px solid #0b9ecc;
    border-bottom: 1px solid #0b9ecc;
    border-right: 1px solid #0b9ecc;
}

.bluetext {
    color: #033164;
}

.HsText {
    color: #bdbdbd;
}

.Currentborder {
    border-top: 1px solid #0b9ecc;
    border-right: 1px solid #0b9ecc;
    border-left: 1px solid #0b9ecc;
    background-image: url(../images/Regedit_Dq.jpg);
}

.nextborder {
    border-top: 1px solid #c5c5c5;
    border-right: 1px solid #c5c5c5;
    background-image: url(../images/Regedit_next.jpg);
}

.regedit_input {
    border: 1px solid #7daeb2;
    height: auto;
}

.px20 {
    font-size: 20px;
}

.button_bg {
    background-image: url('../images/button_bg.gif');
    border: #86b2d1 1px solid;
    color: #333333;
}

.hand {
    cursor: pointer;
    height: 24px;
}


/*表格通用样式*/
.Table_Title_tr {
    background-image: url(../images/tab_bg.gif);
    height: 22px;
}

.Table_Page_td {
    background-image: url(../images/page_bg.gif);
    height: 32px;
    border-left: 1px solid #b4d6e2;
    border-right: 1px solid #b4d6e2;
    border-bottom: 1px solid #b4d6e2;
}

/*路径栏通用样式*/
.map_bg {
    border: 1px #7babe8 solid;
    background-image: url(../images/map_bg.gif);
}

/*流程栏通用样式*/
.lc_bg {
    border: 1px #8ab3ce solid;
    background-image: url(../images/lc.gif);
}

.lc_btn {
    border: 1px #8ab3ce solid;
    background-image: url(../images/lc_btn_bg.gif);
    height: 24px;
}

.lc_btn2 {
    border: 1px #15a4d0 solid;
    background-image: url(../images/lc_btn_bg2.gif);
    height: 24px;
}

/*链接样式*/
A.link_green:link {
    COLOR: #060;
}

A.link_green:visited {
    COLOR: #060;
}

A.link_green:hover {
    COLOR: #c00;
}

A.link_white:link {
    COLOR: #fff;
}

A.link_white:visited {
    COLOR: #fff;
}

A.link_white:hover {
    COLOR: #ff0;
}

A:link {
    COLOR: #036;
    TEXT-DECORATION: none;
}

A:visited {
    COLOR: #036;
    TEXT-DECORATION: none;
}

A:hover {
    COLOR: #c00;
    TEXT-DECORATION: underline;
}

A:active {
    COLOR: #f60;
}

.TableCSS {
    width: "100%";
    border: "0";
    text-align: center;
    padding: "0";
    margin: 0;
    background-color: "#b4d6e2";
}

.tdCSS {
    text-align: left;
}

.btnStyleAddPic {
    background-image: url('./../Images/22.gif');
    background-repeat: no-repeat;
    padding-left: 15px;
}

.btnStyleModifyPic {
    background-image: url('./../Images/33.gif');
    background-repeat: no-repeat;
    padding-left: 15px;
}

.btnStyleModify2Pic {
    background-image: url('./../Images/edit_icon.gif');
    background-repeat: no-repeat;
    padding-left: 15px;
}

.btnStyleDeletePic {
    background-image: url('./../Images/11.gif');
    background-repeat: no-repeat;
    padding-left: 15px;
}


/* CSS Document */

body {
    margin: 0 0 0 0;
}

.button_area {
    padding: 2px 0 5px 0;
    background: #eeedea url(../Images/button_area_back.gif) repeat-x;
    width: 100%;
    border: 1px solid #c9c7c0;
    border-bottom: 1px solid #9a9994;
}

.button {
    background: none;
    overflow: visible;
    text-decoration: none;
    color: #333;
    border: solid #bbbbbb 1px;
    line-height: 15px;
    vertical-align: middle;
    font-style: normal;
    font-variant: normal;
    font-weight: normal;
    font-size: 12px;
    font-family: Arial, Sans-serif;
    height: 23px;
}

.SelectButton {
    overflow: visible;
    font: 12px Arial,Sans-serif;
    text-decoration: none;
    border: solid #8db2e3 1px;
    height: 23px;
    line-height: 19px;
    background-image: url(../Images/blue_back3.gif);
}

.headSPAN {
    color: #15428b;
    font-size: 12px;
    background-color: #cedff5;
}

.headIMG {
    border: 0;
    width: 14px;
    height: 15px;
    margin-left: 5px;
}

.pageTEXT {
    width: 25px;
    margin-top: 5px;
    padding-left: 1px;
}

.buttonTOP {
    margin-top: 5px;
}

.STYLE3 {
    color: #15428b;
    font-size: 12px;
    font-weight: bold;
}

.Table_Title_tr {
    background-image: url(/images/blue_back.gif);
    height: 25px;
    font-weight: bold;
    background-color: #FFFFFF;
    color: #15428b;
    font-size: 12px;
    text-align: center;
    border-spacing: 1px;
    border: 0px solid #8db2e3;
}

.Table_Title_border {
    border-spacing: 1px;
    border: 1px solid #8db2e3;
}

.remark {
    font-size: 12px;
    color: Green;
}




.tr1 {
    background-image: url(../Images/blue_back.gif);
    width: 20px;
    border:0px;
}

.publicpagination {
    background-image: URL(../../images/background_pagination.gif);
    height: 34px;
    font-size: 10pt;
    color: #1379A9;
    border-left: 0px;
    border-right: 0px;
    border-top: 1px solid #1AA4FF;
    border-bottom: 0px;
    width: 100%;
}
