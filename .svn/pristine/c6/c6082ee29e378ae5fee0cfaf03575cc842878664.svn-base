                    <asp:ListItem Value="hb" Text="黑白"></asp:ListItem>
                </asp:DropDownList>
            </td>
            <td class="left_title_1 ">分别率
            </td>
            <td class="left_title_2">
                <uc3:ctrlTextBoxEx ID="fbl" runat="server" CssClass="regedit_input " />
