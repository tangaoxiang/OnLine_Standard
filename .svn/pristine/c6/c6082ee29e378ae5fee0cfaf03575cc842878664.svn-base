<asp:Content ID="Content1" ContentPlaceHolderID="head" runat="server">
    <link href="../CSS/select_file_20140529.css" media="screen" rel="stylesheet" type="text/css" />
    <style type="text/css">
        .promiseInputWidthExt {
            width: 480px;
        }

        .promiseTextAreaExt {
            width: 99%;
            height: 40px;
        }

        .ddlWidthExt {
            width: 150px;
        }      table {
        background-color: #c9c7c0;
        margin-top: 3px;
