            </td>
            <td class="left_title_2">
                <uc3:ctrlTextBoxEx ID="EXPOSURE_MODE" runat="server" CssClass="regedit_input " />
            </td>
        </tr>
        <tr>
            <td class="left_title_1 ">曝光时间
            </td>
            <td class="left_title_2">
                <uc3:ctrlTextBoxEx ID="DATA_EXPOSURE_TIME" runat="server" CssClass="regedit_input " />
            </td>
