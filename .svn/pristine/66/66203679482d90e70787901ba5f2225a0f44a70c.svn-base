
.select-box {
    width: 100px;
    height: 23px;
    background: #fff;
    border: 1px solid #D5D5D5;
}

.select-wrap {
    border: 0;
    position: relative;
    height: 18px;
    overflow: hidden;
}

.countrySelcet {
    border: 1px solid #D5D5D5;
    padding: 4px 3px;
    font-size: 12px;
    height: 34px;
    width: 100px;
    margin: -1px;
}




