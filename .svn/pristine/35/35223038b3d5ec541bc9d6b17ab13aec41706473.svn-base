<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="ChangePasswd.aspx.cs" Inherits="DigiPower.Onlinecol.Standard.Web.ChangePasswd" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
</head>
<body>
    <form id="form1" runat="server">
    <div>
        <div align="center">
            <table width="600" border="1" cellpadding="5" cellspacing="0" style="border: 1px;">
                <tr>
                    <td colspan="3" class="style5">
                        <div align="center">
                            修改密码
                        </div>
                    </td>
                </tr>
                <tr>
                    <td width="24%" class="style6">
                        <div align="left">
                            登录名
                        </div>
                    </td>
                    <td width="32%" class="style1">
                        <asp:Label ID="LB_LOGINNAME" runat="server" Text="LB_LOGINNAME"></asp:Label>
                    </td>
                    <td width="44%" class="style1">&nbsp;
                        
                    </td>
                </tr>
                <tr>
                    <td class="style7">
                        <div align="left">
                            原密码
                        </div>
                    </td>
                    <td class="style2">
                        <asp:TextBox ID="TXT_OLDPASSWD" runat="server" TextMode="Password"></asp:TextBox>
                    </td>
                    <td class="style2">&nbsp;
                        
                    </td>
                </tr>
                <tr>
                    <td class="style8">
                        <div align="left">
                            新密码
                        </div>
                    </td>
                    <td class="style3">
                        <asp:TextBox ID="TXT_NEWPASSWD1" runat="server" TextMode="Password"></asp:TextBox>
                    </td>
                    <td class="style3" rowspan="2">
                        <div align="left">必需有数字加字符，长度最小6位，如：a9k5jk8
                          &nbsp;
                    </div></td>
                </tr>
                <tr>
                    <td class="style9">
                        <div align="left">
                            再次输入新密码
                        </div>
                    </td>
                    <td class="style4">
                        <asp:TextBox ID="TXT_NEWPASSWD2" runat="server" TextMode="Password"></asp:TextBox>
                    </td>
                </tr>
                <tr>
                    <td colspan="3">
                        <div align="center">
                            <asp:Button ID="BTN_SUBMIT" runat="server" Text="  提交  " OnClick="BTN_SUBMIT_Click" />&nbsp;
                            <asp:Button ID="BTN_RELOGIN" runat="server" Text="重新登录" OnClick="BTN_RELOGIN_Click" /></div>
                    </td>
                </tr>
                <tr>
                    <td colspan="3">
                        <p>&nbsp;
                            </p>
                        <p align="left">
                            系统侦测到您的密码不太安全，必码修改后才能继续使用本系统。</p>
                    </td>
                </tr>
            </table>
        </div>
    </div>
    </form>
</body>
</html>
